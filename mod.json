{
	"geode": "4.0.0-beta.1",
	"gd": {
		"win": "2.2074",
		"android": "2.2074",
		"mac": "2.2074",
		"ios": "2.2074"
	},
	"id": "alphalaneous.that_paste_button",
	"name": "That *** Paste Button",
	"version": "v1.0.2",
	"developer": "Alphalaneous",
	"description": "Warns you if you press paste in \"Edit Group\" on a selection of objects or if you press \"Paste State\".",
	"links": {
        "homepage": "https://linktr.ee/Alphalaneous",
        "source": "https://github.com/Alphalaneous/ThatPasteButton",
        "community": "https://discord.gg/Txn2CA5dHF"
    },
	"dependencies": [
        {
            "id": "geode.node-ids",
            "version": ">=v1.13.2",
            "importance": "required"
        }
    ],
	"settings": {
		"toggle-paste-state": {
			"name": "Show Paste State Warning",
			"default": true,
			"type": "bool"
		},
		"toggle-paste-state-group": {
			"name": "Show Paste State Warning on Edit Group popup",
			"default": true,
			"type": "bool"
		},
		"toggle-paste-color": {
			"name": "Show Paste Color Warning",
			"default": true,
			"type": "bool"
		}
	}
}
